{
  "compilerOptions": {
    "baseUrl": "./src"
  },
  "controllerPathGlobs": [
    "src/routes/*Controller.ts"
  ],
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "routes": {
    "basePath": "/api/v1",
    "routesDir": "build"
  },
  "spec": {
    "basePath": "/api/v1",
    "outputDirectory": "build",
    "specVersion": 3
  }
}
